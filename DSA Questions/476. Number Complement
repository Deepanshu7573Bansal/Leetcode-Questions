class Solution {
public:
    int findComplement(int n) {
        if(n==0){
            return 1;
        }
        int power=0,result=0;
        while(n>0){
            if(n%2==0){
                result=result+pow(2,power);
                power++;
            }
            if(n%2==1){
                power++;
            }
            n=n/2;
        }
        return result;
    }
};
