class Solution {
public:
    string reverseVowels(string s) {
        char str[s.length()];
        int index=0;
        for(int i=0;i<s.length();i++){
            if(s[i]=='a'){
                str[index]=s[i];
                index++;
            }else if(s[i]=='e'){
                str[index]=s[i];
                index++;
            }else if(s[i]=='i'){
                str[index]=s[i];
                index++;
            }else if(s[i]=='o'){
                str[index]=s[i];
                index++;
            }else if(s[i]=='u'){
                str[index]=s[i];
                index++;
            }else if(s[i]=='A'){
                str[index]=s[i];
                index++;
            }else if(s[i]=='E'){
                str[index]=s[i];
                index++;
            }else if(s[i]=='I'){
                str[index]=s[i];
                index++;
            }else if(s[i]=='O'){
                str[index]=s[i];
                index++;
            }else if(s[i]=='U'){
                str[index]=s[i];
                index++;
            }
        }

        index=index-1;
        for(int i=0;i<s.length();i++){
            if(s[i]=='a'){
                s[i]=str[index];
                index--;
            }else if(s[i]=='e'){
                s[i]=str[index];
                index--;
            }else if(s[i]=='i'){
                s[i]=str[index];
                index--;
            }else if(s[i]=='o'){
                s[i]=str[index];
                index--;
            }else if(s[i]=='u'){
                s[i]=str[index];
                index--;
            }else if(s[i]=='A'){
                s[i]=str[index];
                index--;
            }else if(s[i]=='E'){
                s[i]=str[index];
                index--;
            }else if(s[i]=='I'){
                s[i]=str[index];
                index--;
            }else if(s[i]=='O'){
                s[i]=str[index];
                index--;
            }else if(s[i]=='U'){
                s[i]=str[index];
                index--;
            }
        }
        return s;
    }
};
